local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "made by nowu_real999", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "DRAGS السحب",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "اقوا رميه Super Throw",
	Callback = function()
      		print("button pressed")loadstring(game:HttpGet("https://rawscripts.net/raw/Fling-Things-and-People-super-throw-open-source-43154"))()
  	end    
})

local Tab = Window:MakeTab({
	Name = "Anti حمايه",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "anti grab مكافحة الاستيلاء",
	Callback = function()
      		print("button pressed")loadstring(game:HttpGet("https://rawscripts.net/raw/Fling-Things-and-People-Anti-Grab-41104"))()
  	end    
})

